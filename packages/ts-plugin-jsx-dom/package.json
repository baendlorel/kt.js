{
  "name": "@ktjs/ts-plugin-jsx-dom",
  "version": "0.1.4",
  "description": "TypeScript language service plugin to show DOM element types for JSX.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "tsserver",
    "plugin",
    "jsx",
    "dom"
  ],
