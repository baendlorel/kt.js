    "name": "Kasukabe Tsumugi",
    "email": "futami16237@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baendlorel/kt.js",
    "directory": "packages/ts-plugin-jsx-dom"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs -w"
  }
}
