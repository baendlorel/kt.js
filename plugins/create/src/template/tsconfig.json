{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "kt.js",
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["vite/client"],
    "plugins": [{ "name": "@ktjs/ts-plugin" }]
  },
  "include": ["src"]
}
