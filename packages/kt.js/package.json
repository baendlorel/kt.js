{
  "name": "kt.js",
  "version": "0.6.14",
  "author": {
    "name": "Kasukabe Tsumugi",
    "email": "futami16237@gmail.com"
  },
  "description": "A simple and easy-to-use web framework, never re-render. Abbreviation of Kasukabe Tsumugi.",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/baendlorel/kt.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/baendlorel/kt.js",
    "directory": "packages/kt.js"
  },
  "keywords": [
    "framework",
    "web",
    "typescript",
    "javascript"
  ],
  "license": "MIT",
  "dependencies": {
    "@ktjs/core": "workspace:^*",
    "@ktjs/jsx": "workspace:^*",
    "@ktjs/shortcuts": "workspace:^*"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "dev": "rollup -c rollup.config.mjs -w"
  }
}